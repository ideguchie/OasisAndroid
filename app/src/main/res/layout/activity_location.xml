<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.app.oasis.oasismobileapp.LocationActivity">

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Renton"
        android:id="@+id/rentonButton"
        android:layout_alignParentTop="true"
        android:background="@drawable/custombutton"
        android:layout_centerHorizontal="true"
        android:onClick="openRenton" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Seattle"
        android:id="@+id/seattleButton"
        android:layout_below="@+id/rentonButton"
        android:background="@drawable/custombutton"
        android:layout_centerHorizontal="true"
        android:onClick="openSeattle" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="UW"
        android:id="@+id/uwButton"
        android:layout_below="@+id/seattleButton"
        android:background="@drawable/custombutton"
        android:layout_centerHorizontal="true"
        android:onClick="openUW" />
</RelativeLayout>
